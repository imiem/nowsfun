{
    "manifest_version": 2,
    "name": "nowsfun",
    "description": "nowsfun",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "content_scripts": 
    [
        {
            "matches": ["<all_urls>"],
            "js": ["assets/newtab.js"],
            "run_at": "document_start"
        }
    ]
}